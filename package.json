{
  "name": "apex-launcher",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "test": "karma start karma.conf.js",
    "start": "webpack -w --mode=development --devtool=cheap-module-eval-source-map",
    "local": "webpack-dev-server --hot --mode=development --devtool=cheap-module-eval-source-map",
    "build": "webpack --mode=production --devtool=source-map --bail",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "tscheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Semibold/Apex-Launcher.git"
  },
  "author": "Aqours",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Semibold/Apex-Launcher/issues"
  },
  "homepage": "https://github.com/Semibold/Apex-Launcher#readme",
  "prettier": {
    "singleQuote": false,
    "trailingComma": "all"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "browserslist": [
    "Firefox ESR",
    "> 1% in CN",
    "> 2% in US",
    "last 5 versions",
    "not ie 6-11"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "browser-cookies": "1.2.0",
    "events": "3.1.0",
    "promise-polyfill": "8.1.3",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "tslib": "1.11.1",
    "url-parse": "1.4.7"
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@types/chai": "4.2.11",
    "@types/event-hooks-webpack-plugin": "2.0.0",
    "@types/events": "3.0.0",
    "@types/git-rev-sync": "1.12.0",
    "@types/gulp": "4.0.6",
    "@types/karma": "4.4.0",
    "@types/mocha": "7.0.2",
    "@types/node": "13.9.3",
    "@types/react": "16.9.25",
    "@types/react-dom": "16.9.5",
    "@types/rimraf": "3.0.0",
    "@types/sinon": "7.5.2",
    "@types/terser-webpack-plugin": "2.2.0",
    "@types/url-parse": "1.4.3",
    "@types/webpack": "4.41.8",
    "@types/webpack-bundle-analyzer": "2.13.3",
    "@types/webpack-dev-server": "3.10.1",
    "autoprefixer": "9.7.4",
    "chai": "4.2.0",
    "css-loader": "3.4.2",
    "cssnano": "4.1.10",
    "event-hooks-webpack-plugin": "2.1.6",
    "file-loader": "6.0.0",
    "git-rev-sync": "2.0.0",
    "gulp": "4.0.2",
    "husky": "4.2.3",
    "karma": "4.4.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.5",
    "karma-typescript": "5.0.1",
    "less": "3.11.1",
    "less-loader": "5.0.0",
    "mocha": "7.1.1",
    "postcss-loader": "3.0.0",
    "prettier": "2.0.1",
    "pretty-quick": "2.0.1",
    "rimraf": "3.0.2",
    "shell-env": "3.0.0",
    "sinon": "9.0.1",
    "style-loader": "1.1.3",
    "terser-webpack-plugin": "2.3.5",
    "ts-loader": "6.2.2",
    "tslint": "6.1.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.8.3",
    "url-loader": "4.0.0",
    "webpack": "4.42.0",
    "webpack-bundle-analyzer": "3.6.1",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3",
    "worker-loader": "2.0.0"
  }
}
