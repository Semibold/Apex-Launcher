{
  "name": "apex-launcher",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@8.10.2",
  "scripts": {
    "prepare": "node ./.husky/setup.js",
    "husky:pre-commit": "lint-staged",
    "husky:commit-msg": "commitlint --edit ${1}",
    "test": "pnpm run eslint && pnpm run tscheck",
    "eslint": "eslint '**/*.{js,jsx,ts,tsx}' --quiet",
    "tscheck": "pnpm -r --parallel exec tsc --noEmit",
    "tscheck:whole": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Semibold/Apex-Launcher.git"
  },
  "author": "Aqours",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Semibold/Apex-Launcher/issues"
  },
  "homepage": "https://github.com/Semibold/Apex-Launcher#readme",
  "browserslist": [
    "Firefox ESR",
    "> 1% in CN",
    "> 2% in US",
    "last 5 versions",
    "not ie 6-11"
  ],
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,vue}": "prettier --write"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@apex/webpack-config": "workspace:*",
    "@changesets/cli": "2.26.2",
    "@commitlint/cli": "18.2.0",
    "@commitlint/config-conventional": "18.1.0",
    "@types/event-hooks-webpack-plugin": "2.2.4",
    "@types/git-rev-sync": "2.0.1",
    "@types/node": "20.8.10",
    "@types/rimraf": "3.0.2",
    "@types/shared-types": "workspace:*",
    "@types/terser-webpack-plugin": "5.0.4",
    "@types/webpack-bundle-analyzer": "4.6.2",
    "@typescript-eslint/eslint-plugin": "6.9.1",
    "@typescript-eslint/parser": "6.9.1",
    "autoprefixer": "10.4.16",
    "ci-info": "4.0.0",
    "cross-env": "7.0.3",
    "css-loader": "6.8.1",
    "cssnano": "6.0.1",
    "dotenv": "16.3.1",
    "esbuild-loader": "4.0.2",
    "eslint": "8.52.0",
    "eslint-config-prettier": "9.0.0",
    "event-hooks-webpack-plugin": "2.3.0",
    "git-rev-sync": "3.0.2",
    "husky": "8.0.3",
    "less": "4.2.0",
    "less-loader": "11.1.3",
    "lint-staged": "15.0.2",
    "moment": "2.29.4",
    "npm-check-updates": "16.14.6",
    "postcss": "8.4.31",
    "postcss-custom-properties": "13.3.2",
    "postcss-loader": "7.3.3",
    "postcss-preset-env": "9.3.0",
    "prettier": "3.0.3",
    "rimraf": "5.0.5",
    "semver": "7.5.4",
    "style-loader": "3.3.3",
    "svg-inline-loader": "0.8.2",
    "svgo": "3.0.2",
    "svgo-loader": "4.0.0",
    "terser-webpack-plugin": "5.3.9",
    "ts-loader": "9.5.0",
    "ts-node": "10.9.1",
    "tslib": "2.6.2",
    "typescript": "5.2.2",
    "webpack": "5.89.0",
    "webpack-bundle-analyzer": "4.9.1",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "4.15.1",
    "webpack-merge": "5.10.0",
    "webpack-retry-chunk-load-plugin": "3.1.1",
    "webpackbar": "5.0.2"
  }
}
