{
  "name": "apex-launcher",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "test": "karma start karma.conf.js",
    "start": "webpack -w --mode=development --devtool=cheap-module-eval-source-map",
    "local": "webpack-dev-server --hot --mode=development --devtool=cheap-module-eval-source-map",
    "build": "webpack --mode=production --devtool=source-map --bail",
    "build:dll": "webpack --config webpack.dll.config.js --mode=production --devtool=source-map --bail",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Semibold/Apex-Launcher.git"
  },
  "author": "Aqours",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Semibold/Apex-Launcher/issues"
  },
  "homepage": "https://github.com/Semibold/Apex-Launcher#readme",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "browserslist": [
    "Firefox ESR",
    "> 1% in CN",
    "> 2% in US",
    "last 5 versions",
    "not ie 6-11"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "browser-cookies": "1.2.0",
    "events": "3.0.0",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "tslib": "1.10.0",
    "url-parse": "1.4.7"
  },
  "devDependencies": {
    "@commitlint/cli": "8.1.0",
    "@commitlint/config-conventional": "8.1.0",
    "@types/chai": "4.2.0",
    "@types/event-hooks-webpack-plugin": "2.0.0",
    "@types/events": "3.0.0",
    "@types/gulp": "4.0.6",
    "@types/karma": "3.0.3",
    "@types/mocha": "5.2.7",
    "@types/node": "12.7.1",
    "@types/react": "16.9.1",
    "@types/react-dom": "16.8.5",
    "@types/sinon": "7.0.13",
    "@types/terser-webpack-plugin": "1.2.1",
    "@types/url-parse": "1.4.3",
    "@types/webpack": "4.32.1",
    "@types/webpack-bundle-analyzer": "2.13.2",
    "@types/webpack-dev-server": "3.1.7",
    "ansi-colors": "4.1.1",
    "autoprefixer": "9.6.1",
    "chai": "4.2.0",
    "css-loader": "3.2.0",
    "cssnano": "4.1.10",
    "event-hooks-webpack-plugin": "2.1.4",
    "file-loader": "4.2.0",
    "git-rev-sync": "1.12.0",
    "gulp": "4.0.2",
    "husky": "3.0.3",
    "karma": "4.2.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.5",
    "karma-typescript": "4.1.1",
    "less": "3.9.0",
    "less-loader": "5.0.0",
    "mocha": "6.2.0",
    "postcss-loader": "3.0.0",
    "prettier": "1.18.2",
    "pretty-quick": "1.11.1",
    "rimraf": "2.7.0",
    "shell-env": "3.0.0",
    "sinon": "7.4.1",
    "style-loader": "1.0.0",
    "ts-loader": "6.0.4",
    "tslint": "5.18.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.5.3",
    "url-loader": "2.1.0",
    "webpack": "4.39.2",
    "webpack-bundle-analyzer": "3.4.1",
    "webpack-cli": "3.3.6",
    "webpack-dev-server": "3.8.0"
  }
}
