{
  "name": "apex-launcher",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "test": "tsc --noEmit",
    "start": "webpack -w --mode=development --devtool=eval-cheap-module-source-map",
    "local": "webpack serve --hot --mode=development --devtool=eval-cheap-module-source-map",
    "build": "webpack --mode=production --devtool=source-map --bail"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Semibold/Apex-Launcher.git"
  },
  "author": "Aqours",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Semibold/Apex-Launcher/issues"
  },
  "homepage": "https://github.com/Semibold/Apex-Launcher#readme",
  "prettier": {
    "singleQuote": false,
    "trailingComma": "all"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "browserslist": [
    "Firefox ESR",
    "> 1% in CN",
    "> 2% in US",
    "last 5 versions",
    "not ie 6-11"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "events": "3.3.0",
    "mobx": "6.3.2",
    "promise-polyfill": "8.2.0",
    "qs": "6.10.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "tslib": "2.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@types/event-hooks-webpack-plugin": "2.2.1",
    "@types/events": "3.0.0",
    "@types/git-rev-sync": "2.0.0",
    "@types/node": "15.6.1",
    "@types/react": "17.0.8",
    "@types/react-dom": "17.0.5",
    "@types/rimraf": "3.0.0",
    "@types/terser-webpack-plugin": "5.0.3",
    "@types/webpack": "5.28.0",
    "@types/webpack-bundle-analyzer": "4.4.0",
    "@types/webpack-dev-server": "3.11.4",
    "autoprefixer": "10.2.6",
    "css-loader": "5.2.6",
    "cssnano": "5.0.5",
    "event-hooks-webpack-plugin": "2.2.0",
    "file-loader": "6.2.0",
    "git-rev-sync": "3.0.1",
    "husky": "6.0.0",
    "less": "4.1.1",
    "less-loader": "9.0.0",
    "moment": "2.29.1",
    "postcss": "8.3.0",
    "postcss-loader": "5.3.0",
    "prettier": "2.3.0",
    "pretty-quick": "3.1.0",
    "rimraf": "3.0.2",
    "shell-env": "3.0.1",
    "style-loader": "2.0.0",
    "terser-webpack-plugin": "5.1.3",
    "ts-loader": "9.2.2",
    "typescript": "4.3.2",
    "url-loader": "4.1.1",
    "webpack": "5.38.1",
    "webpack-bundle-analyzer": "4.4.2",
    "webpack-cli": "4.7.0",
    "webpack-dev-server": "3.11.2",
    "webpack-retry-chunk-load-plugin": "2.2.0",
    "worker-loader": "3.0.8"
  }
}
