    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@types/chai": "^4.1.7",
    "@types/events": "^1.2.0",
    "@types/gulp": "^4.0.5",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/sinon": "^7.0.3",
    "@types/url-parse": "^1.4.2",
    "@types/webpack": "^4.4.22",
    "autoprefixer": "^9.4.4",
    "babel-loader": "^8.0.5",
    "chai": "^4.2.0",
