{
  "name": "apex-launcher",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "test": "npm run eslint && npm run tscheck",
    "start": "webpack -w --mode=development --devtool=eval-cheap-module-source-map",
    "local": "webpack serve --hot --mode=development --devtool=eval-cheap-module-source-map",
    "build": "webpack --mode=production --devtool=source-map --bail",
    "eslint": "eslint '**/*.{js,jsx,ts,tsx}' --quiet",
    "tscheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Semibold/Apex-Launcher.git"
  },
  "author": "Aqours",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Semibold/Apex-Launcher/issues"
  },
  "homepage": "https://github.com/Semibold/Apex-Launcher#readme",
  "prettier": {
    "singleQuote": false,
    "trailingComma": "all"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "browserslist": [
    "Firefox ESR",
    "> 1% in CN",
    "> 2% in US",
    "last 5 versions",
    "not ie 6-11"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "events": "3.3.0",
    "mobx": "6.6.2",
    "promise-polyfill": "8.2.3",
    "qs": "6.11.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tslib": "2.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "@types/event-hooks-webpack-plugin": "2.2.2",
    "@types/events": "3.0.0",
    "@types/git-rev-sync": "2.0.0",
    "@types/node": "18.8.4",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@types/rimraf": "3.0.2",
    "@types/terser-webpack-plugin": "5.0.4",
    "@types/webpack": "5.28.0",
    "@types/webpack-bundle-analyzer": "4.6.0",
    "@types/webpack-dev-server": "4.7.1",
    "@typescript-eslint/eslint-plugin": "5.40.0",
    "@typescript-eslint/parser": "5.40.0",
    "autoprefixer": "10.4.12",
    "css-loader": "6.7.1",
    "cssnano": "5.1.13",
    "eslint": "8.25.0",
    "eslint-config-prettier": "8.5.0",
    "event-hooks-webpack-plugin": "2.2.0",
    "file-loader": "6.2.0",
    "git-rev-sync": "3.0.2",
    "husky": "8.0.1",
    "less": "4.1.3",
    "less-loader": "11.1.0",
    "moment": "2.29.4",
    "npm-check-updates": "16.3.11",
    "postcss": "8.4.17",
    "postcss-loader": "7.0.1",
    "prettier": "2.7.1",
    "pretty-quick": "3.1.3",
    "rimraf": "3.0.2",
    "style-loader": "3.3.1",
    "terser-webpack-plugin": "5.3.6",
    "ts-loader": "9.4.1",
    "typescript": "4.8.4",
    "url-loader": "4.1.1",
    "webpack": "5.74.0",
    "webpack-bundle-analyzer": "4.6.1",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.11.1",
    "webpack-retry-chunk-load-plugin": "3.1.1"
  }
}
