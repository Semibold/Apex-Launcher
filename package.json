{
  "name": "apex-launcher",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@8.3.0",
  "scripts": {
    "prepare": "node -e '!require(`ci-info`).isCI && require(`husky`).install()'",
    "husky:pre-commit": "pretty-quick --staged",
    "husky:commit-msg": "commitlint --edit ${1}",
    "test": "pnpm run eslint && pnpm run tscheck",
    "eslint": "eslint '**/*.{js,jsx,ts,tsx}' --quiet",
    "tscheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Semibold/Apex-Launcher.git"
  },
  "author": "Aqours",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Semibold/Apex-Launcher/issues"
  },
  "homepage": "https://github.com/Semibold/Apex-Launcher#readme",
  "browserslist": [
    "Firefox ESR",
    "> 1% in CN",
    "> 2% in US",
    "last 5 versions",
    "not ie 6-11"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@apex/webpack-config": "workspace:*",
    "@babel/cli": "7.21.0",
    "@babel/core": "7.21.4",
    "@babel/preset-env": "7.21.4",
    "@babel/runtime": "7.21.0",
    "@changesets/cli": "2.26.1",
    "@commitlint/cli": "17.6.1",
    "@commitlint/config-conventional": "17.6.1",
    "@types/event-hooks-webpack-plugin": "2.2.2",
    "@types/events": "3.0.0",
    "@types/git-rev-sync": "2.0.0",
    "@types/node": "18.15.11",
    "@types/react": "18.0.37",
    "@types/react-dom": "18.0.11",
    "@types/rimraf": "3.0.2",
    "@types/terser-webpack-plugin": "5.0.4",
    "@types/webpack-bundle-analyzer": "4.6.0",
    "@types/webpack-dev-server": "4.7.1",
    "@typescript-eslint/eslint-plugin": "5.59.0",
    "@typescript-eslint/parser": "5.59.0",
    "autoprefixer": "10.4.14",
    "babel-loader": "9.1.2",
    "ci-info": "3.8.0",
    "cross-env": "7.0.3",
    "css-loader": "6.7.3",
    "cssnano": "6.0.0",
    "dotenv": "16.0.3",
    "eslint": "8.38.0",
    "eslint-config-prettier": "8.8.0",
    "event-hooks-webpack-plugin": "2.3.0",
    "git-rev-sync": "3.0.2",
    "husky": "8.0.3",
    "less": "4.1.3",
    "less-loader": "11.1.0",
    "moment": "2.29.4",
    "npm-check-updates": "16.10.8",
    "postcss": "8.4.22",
    "postcss-custom-properties": "13.1.5",
    "postcss-loader": "7.2.4",
    "postcss-preset-env": "8.3.2",
    "prettier": "2.8.7",
    "pretty-quick": "3.1.3",
    "rimraf": "5.0.0",
    "semver": "7.5.0",
    "style-loader": "3.3.2",
    "svg-inline-loader": "0.8.2",
    "svgo": "3.0.2",
    "svgo-loader": "4.0.0",
    "terser-webpack-plugin": "5.3.7",
    "ts-loader": "9.4.2",
    "tslib": "2.5.0",
    "typescript": "5.0.4",
    "webpack": "5.79.0",
    "webpack-bundle-analyzer": "4.8.0",
    "webpack-cli": "5.0.1",
    "webpack-dev-server": "4.13.3",
    "webpack-merge": "5.8.0",
    "webpack-retry-chunk-load-plugin": "3.1.1",
    "webpackbar": "5.0.2"
  }
}
