{
  "name": "launcher",
  "version": "1.0.0",
  "private": true,
  "types": "src/index.ts",
  "scripts": {
    "test": "npm run karma && npm run protractor",
    "karma": "karma start karma.conf.js",
    "protractor": "webdriver-manager update && tsc -p test/e2e && protractor protractor.conf.js",
    "start": "webpack -w --mode=development --devtool=cheap-module-eval-source-map",
    "local": "webpack-dev-server --hot --mode=development --devtool=cheap-module-eval-source-map",
    "build": "webpack --mode=production --devtool=source-map",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Semibold/launcher.git"
  },
  "author": "Aqours",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Semibold/launcher/issues"
  },
  "homepage": "https://github.com/Semibold/launcher#readme",
  "engines": {
    "node": ">=8.9.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "browserslist": [
    "Firefox ESR",
    "> 1% in CN",
    "> 2% in US",
    "last 3 versions",
    "not ie 6-11"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "browser-cookies": "^1.2.0",
    "events": "^3.0.0",
