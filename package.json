{
  "name": "apex-launcher",
  "version": "1.0.0",
  "private": true,
  "types": "src/index.ts",
  "scripts": {
    "test": "karma start karma.conf.js",
    "start": "webpack -w --mode=development --devtool=cheap-module-eval-source-map",
    "local": "webpack-dev-server --hot --mode=development --devtool=cheap-module-eval-source-map",
    "build": "webpack --mode=production --devtool=source-map --bail --display=errors-only",
    "build:dll": "webpack --config webpack.dll.config.js --mode=production --devtool=source-map --bail",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Semibold/Apex-Launcher.git"
  },
  "author": "Aqours",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Semibold/Apex-Launcher/issues"
  },
  "homepage": "https://github.com/Semibold/Apex-Launcher#readme",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "browserslist": [
    "Firefox ESR",
    "> 1% in CN",
    "> 2% in US",
    "last 5 versions",
    "not ie 6-11"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "browser-cookies": "1.2.0",
    "events": "3.0.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "tslib": "1.9.3",
    "url-parse": "1.4.6"
  },
  "optionalDependencies": {
    "@commitlint/cli": "7.5.2",
    "@commitlint/config-conventional": "7.5.0",
    "husky": "1.3.1",
    "prettier": "1.17.0",
    "pretty-quick": "1.10.0"
  },
  "devDependencies": {
    "@types/chai": "4.1.7",
    "@types/event-hooks-webpack-plugin": "2.0.0",
    "@types/events": "3.0.0",
    "@types/gulp": "4.0.6",
    "@types/karma": "3.0.2",
    "@types/mocha": "5.2.6",
    "@types/node": "11.13.4",
    "@types/react": "16.8.13",
    "@types/react-dom": "16.8.4",
    "@types/sinon": "7.0.11",
    "@types/terser-webpack-plugin": "1.2.1",
    "@types/url-parse": "1.4.3",
    "@types/webpack": "4.4.27",
    "@types/webpack-bundle-analyzer": "2.13.1",
    "@types/webpack-dev-server": "3.1.5",
    "ansi-colors": "3.2.4",
    "autoprefixer": "9.5.1",
    "chai": "4.2.0",
    "css-loader": "2.1.1",
    "cssnano": "4.1.10",
    "event-hooks-webpack-plugin": "2.1.1",
    "file-loader": "3.0.1",
    "git-rev-sync": "1.12.0",
    "gulp": "4.0.0",
    "karma": "4.0.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.5",
    "karma-typescript": "4.0.0",
    "less": "3.9.0",
    "less-loader": "4.1.0",
    "link-parent-bin": "1.0.0",
    "mocha": "6.1.3",
    "postcss-loader": "3.0.0",
    "rimraf": "2.6.3",
    "shell-env": "3.0.0",
    "sinon": "7.3.1",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "1.2.3",
    "ts-loader": "5.3.3",
    "ts-node": "8.1.0",
    "tslint": "5.15.0",
    "typescript": "3.4.3",
    "url-loader": "1.1.2",
    "webpack": "4.30.0",
    "webpack-bundle-analyzer": "3.3.2",
    "webpack-cli": "3.3.0",
    "webpack-dev-server": "3.3.1"
  }
}
