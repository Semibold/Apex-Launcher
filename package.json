{
  "name": "apex-launcher",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@7.27.0",
  "scripts": {
    "prepare": "node -e '!require(`ci-info`).isCI && require(`husky`).install()'",
    "husky:pre-commit": "pretty-quick --staged",
    "husky:commit-msg": "commitlint --edit ${1}",
    "test": "pnpm run eslint && pnpm run tscheck",
    "start": "webpack -w --mode=development --devtool=eval-cheap-module-source-map",
    "local": "webpack serve --hot --mode=development --devtool=eval-cheap-module-source-map",
    "build": "webpack --mode=production",
    "eslint": "eslint '**/*.{js,jsx,ts,tsx}' --quiet",
    "tscheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Semibold/Apex-Launcher.git"
  },
  "author": "Aqours",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Semibold/Apex-Launcher/issues"
  },
  "homepage": "https://github.com/Semibold/Apex-Launcher#readme",
  "browserslist": [
    "Firefox ESR",
    "> 1% in CN",
    "> 2% in US",
    "last 5 versions",
    "not ie 6-11"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "axios": "1.3.3",
    "events": "3.3.0",
    "js-cookie": "3.0.1",
    "lodash-es": "4.17.21",
    "qs": "6.11.0"
  },
  "devDependencies": {
    "@apex/webpack-config": "workspace:*",
    "@babel/cli": "7.21.0",
    "@babel/core": "7.21.0",
    "@babel/preset-env": "7.20.2",
    "@babel/runtime": "7.21.0",
    "@changesets/cli": "2.26.0",
    "@commitlint/cli": "17.4.4",
    "@commitlint/config-conventional": "17.4.4",
    "@types/event-hooks-webpack-plugin": "2.2.2",
    "@types/events": "3.0.0",
    "@types/git-rev-sync": "2.0.0",
    "@types/node": "18.14.4",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/rimraf": "3.0.2",
    "@types/terser-webpack-plugin": "5.0.4",
    "@types/webpack-bundle-analyzer": "4.6.0",
    "@types/webpack-dev-server": "4.7.1",
    "@typescript-eslint/eslint-plugin": "5.54.0",
    "@typescript-eslint/parser": "5.54.0",
    "autoprefixer": "10.4.13",
    "babel-loader": "9.1.2",
    "ci-info": "3.8.0",
    "cross-env": "7.0.3",
    "css-loader": "6.7.3",
    "cssnano": "5.1.15",
    "depcheck": "1.4.3",
    "dotenv": "16.0.3",
    "eslint": "8.35.0",
    "eslint-config-prettier": "8.6.0",
    "event-hooks-webpack-plugin": "2.2.0",
    "git-rev-sync": "3.0.2",
    "husky": "8.0.3",
    "less": "4.1.3",
    "less-loader": "11.1.0",
    "moment": "2.29.4",
    "npm-check-updates": "16.7.10",
    "postcss": "8.4.21",
    "postcss-custom-properties": "13.1.3",
    "postcss-loader": "7.0.2",
    "postcss-preset-env": "8.0.1",
    "prettier": "2.8.4",
    "pretty-quick": "3.1.3",
    "rimraf": "4.2.0",
    "semver": "7.3.8",
    "style-loader": "3.3.1",
    "svg-inline-loader": "0.8.2",
    "svgo": "3.0.2",
    "svgo-loader": "4.0.0",
    "terser-webpack-plugin": "5.3.6",
    "ts-loader": "9.4.2",
    "tslib": "2.5.0",
    "typescript": "4.9.5",
    "webpack": "5.75.0",
    "webpack-bundle-analyzer": "4.8.0",
    "webpack-cli": "5.0.1",
    "webpack-dev-server": "4.11.1",
    "webpack-merge": "5.8.0",
    "webpack-retry-chunk-load-plugin": "3.1.1",
    "webpackbar": "5.0.2"
  }
}
