    "istanbul-instrumenter-loader": "^3.0.1",
    "karma": "^3.1.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-mocha": "^1.3.0",
    "karma-webpack": "^3.0.5",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "mocha": "^5.2.0",
    "postcss-loader": "^3.0.0",
