{
  "packages": [
    "packages/*"
  ],
  "command": {
    "add": {
      "exact": true
    },
    "create": {
      "homepage": "https://github.com/Semibold/Apex-Launcher",
      "license": "UNLICENSED"
    },
    "version": {
      "exact": true,
      "allowBranch": "master",
      "message": "chore(release): publish %v"
    }
  },
  "npmClient": "npm",
  "ignoreChanges": [
    "**/dist/**",
    "**/coverage/**",
    "**/LICENSE",
    "**/*.txt",
    "**/*.log",
    "**/*.md",
    "**/.*",
    "**/yarn.lock",
    "**/package-lock.json"
  ],
  "version": "1.0.0"
}
