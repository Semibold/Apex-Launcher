{
  "compileOnSave": false,
  "ts-node": {
    "transpileOnly": true,
    "compilerOptions": {
      "module": "CommonJS",
      "target": "ESNext"
    }
  },
  "compilerOptions": {
    "pretty": true,
    "strict": false,
    "sourceMap": true,
    "skipLibCheck": true,
    "importHelpers": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "downlevelIteration": true,
    "target": "es5",
    "module": "esnext",
    "moduleResolution": "node"
  }
}
